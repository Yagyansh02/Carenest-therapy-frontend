# Production API Configuration
VITE_API_BASE_URL=https://api.carenest.com/api/v1

# Environment
VITE_ENV=production

# App Configuration
VITE_APP_NAME=CareNest
VITE_APP_VERSION=1.0.0

# Token Configuration (in milliseconds)
VITE_TOKEN_REFRESH_THRESHOLD=300000
